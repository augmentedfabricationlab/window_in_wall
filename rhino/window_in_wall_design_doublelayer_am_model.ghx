                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st

import sys, os
am_hackathon_path = os.path.abspath(os.path.join(ghdoc.Path,"../../src"))
if am_hackathon_path not in sys.path:
    sys.path.append(am_hackathon_path)

from compas.geometry import Frame
from compas.geometry import Translation
from compas.geometry import Transformation
from compas.geometry import Vector
from compas_ghpython import update_component
from mobile_additive_manufacturing.control import URScript_Extrusion
from mobile_additive_manufacturing.control import set_extruder_data
from ur_fabrication_control.direct_control import URScript
from ur_fabrication_control.direct_control.mixins.airpick_mixins import AirpickMixins
from ur_fabrication_control.direct_control.fabrication_process import Fabrication

ur_ip = st["ur_ip"]
ur_port = st["ur_port"]
server_ip = st["server_address"]
server_port = st["server_port"]
ext_ip = st["ext_ip"]
ext_port = st["ext_port"]


def create_tasks_from_am_model(am_model):
    for key, layer in am_model.layers():
        if key == 0:
            toggle_extruder = True
        elif key == am_model.number_of_layers()-1:
            toggle_extruder = False
        else:
            toggle_extruder = None
        exit_message = "Layer{}_completed".format(key)
        layer_urscript = get_layer_ur_script(layer, toggle_extruder, exit_message)
        fabrication.add_task(layer_urscript, exit_message)

def get_frames_in_RCS(frames):
    return [robot.to_local_coordinates(frame) for frame in frames]

def get_layer_ur_script(layer, toggle_extruder, exit_message):
    ur_script = URScript_Extrusion(ur_ip=ur_ip, ur_port=ur_port, ext_ip=ext_ip, ext_port=ext_port)
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
    
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    RCS_frames = get_frames_in_RCS([node.frame for key, node in layer.nodes()])
    first = False
    for key, node in layer.nodes():
        if key in [0]:
            ur_script.move_linear(robot.to_local_coordinates(node.frame), velocity=ur_speed, radius=node.radius)
            if (toggle_extruder == True) and (toggle_extruder != None) and include_extruder:
                toggle_extrusion(ur_script, 1)
        else:
            ur_script.move_process(robot.to_local_coordinates(node.frame), ur_speed, node.radius)
    else:
        if (toggle_extruder == False) and (toggle_extruder != None) and include_extruder:
            toggle_extrusion(ur_script, 0)
    # Signify ready
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    ur_script.socket_send_line_string(exit_message, socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")
    #Finalize script
    ur_script.end()
    ur_script.generate()
    return ur_script

def get_stop_ur_script(safety_plane):
    safety_frame_WCS = Frame(safety_plane.Origin, safety_plane.XAxis, safety_plane.YAxis)
    safety_frame_RCS = robot.to_local_coordinates(safety_frame_WCS)
    
    ur_script = URScript_Extrusion(ur_ip=ur_ip, ur_port=ur_port, ext_ip=ext_ip, ext_port=ext_port)
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
    
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    if include_extruder:
        toggle_extrusion(ur_script, 0)
    ur_script.move_linear(safety_frame_RCS, velocity=ur_speed, radius=0)
    ur_script.end()
    ur_script.generate()
    return ur_script

