def project_crvs(crvs, mesh, direction=rg.Vector3d.YAxis, offset=0, reversed=False):
    proj_crvs = []
    for crv in crvs:
        proj_crv = rg.Curve.ProjectToMesh(crv, mesh, direction, 0.001)[0]
        crv_end_pts = ghcomp.EndPoints(crv)
        proj_end_pts = ghcomp.EndPoints(proj_crv)
        if Vector.from_start_end(crv_end_pts[0], proj_end_pts[0]).length&gt;0.2:
            proj_crv.Reverse()
        if reversed:
            proj_crv.Reverse()
        proj_crv.Translate(0,offset,0)
        proj_crvs.append(proj_crv)
    return proj_crvs

