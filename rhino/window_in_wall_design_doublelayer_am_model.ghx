                    <item name="CodeInput" type_name="gh_string" type_code="10">import os
from scriptcontext import sticky as st

import compas

from compas.robots import RobotModel
from compas.robots import LocalPackageMeshLoader

from compas_fab.robots import Robot
from compas_fab.robots import RobotSemantics
from compas_fab.ghpython import RobotArtist

compas.PRECISION = '12f'

# store robot in component
guid = str(ghenv.Component.InstanceGuid)
robot_key = "robot"

if robot_key not in st:
    st[robot_key] = None

print loaders
if load:
    model = RobotModel.from_urdf_file(urdf_filename)
    
    model.load_geometry(*loaders)
    artist = RobotArtist(model)
    semantics = RobotSemantics.from_srdf_file(srdf_filename, model)
    robot = Robot(model, artist, semantics, client=None)
    
    st[robot_key] = robot

robot = st[robot_key]


if robot:
    robot.scale(scale_factor)</item>
