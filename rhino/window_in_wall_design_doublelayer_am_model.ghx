                    <item name="CodeInput" type_name="gh_string" type_code="10">import time
from scriptcontext import sticky as st
from mobile_additive_manufacturing.control.clay_extruder_control import ExtruderClient

ext_ip = st["ext_ip"]
ext_port = st["ext_port"]

ec = ExtruderClient(ext_ip, ext_port)
wait_for_response = False

if stop:
    ec.connect()
    time.sleep(0.2)
    ec.send_stop(wait_for_response)
    time.sleep(0.2)
    ec.close()
</item>
