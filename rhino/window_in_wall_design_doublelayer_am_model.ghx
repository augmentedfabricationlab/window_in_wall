def layers_from_curves(curves, div, mir_curves=None, alternate=False, reversed=False):
    layers = []
    for j, crv in enumerate(curves):
        nodes = []
        crv_length = ghcomp.Length(crv)
        div_sections = int(crv_length/div)
        (pts, tans, pars) = ghcomp.DivideCurve(crv, div_sections, False)
        
        for i,pt in enumerate(pts):
            frame = Frame(pt, -rg.Vector3d(tans[i].X, 0, tans[i].Z), -Vector.Yaxis())
            nodes.append(Node(frame, radius=max_r))
        if not reversed:
            nodes.reverse()
        
        if mir_curves is not None:
            (mir_pts, mir_tans, mir_pars) = ghcomp.DivideCurve(mir_curves[j], div_sections, False)
            mir_nodes = []
            for i,pt in enumerate(mir_pts):
                frame = Frame(pt, -rg.Vector3d(tans[i].X, 0, tans[i].Z), -Vector.Yaxis())
                mir_nodes.append(Node(frame, radius=max_r))
            if reversed:
                mir_nodes.reverse()
            nodes.extend(mir_nodes)
        if alternate and j%2==0:
            nodes.reverse()
        layers.append(Layer.from_nodes(nodes))
    return layers

