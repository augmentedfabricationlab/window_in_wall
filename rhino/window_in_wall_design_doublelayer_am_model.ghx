                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math
from scriptcontext import sticky as st


from compas.geometry import Frame
from compas.geometry import Transformation
from compas.geometry import Scale
from compas.geometry import Point

from compas_ghpython import draw_frame

from am_information_model.model import AMModel
from am_information_model.model import Layer

import Rhino.Geometry as rg

# initialize an empty assembly class

if "am_model" not in st:
    st["am_model"] = None

cp = centerplane
cf = Frame(cp.Origin, cp.XAxis, cp.YAxis)

S = Scale.from_factors([scale]*3, Frame.worldXY())
T = Transformation.from_frame_to_frame(Frame.worldXY(), cf)

def add_layers_to_am_model(amm, layers):
    for layer in layers:
        new_layer = layer.transformed(S)
        new_layer.transform(T)
        amm.add_layer(new_layer)

if create:
    new_amm = AMModel()
    add_layers_to_am_model(new_amm, layers_left)
    add_layers_to_am_model(new_amm, layers_right)
    add_layers_to_am_model(new_amm, layers_top)
    st["am_model"] = new_amm

am_model = st["am_model"]</item>
